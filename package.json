{
  "name": "plume2",
  "version": "0.1.6",
  "description": "a lightweight of flux for mobile web",
  "main": "./dist/index.js",
  "types": "./typings/index.d.ts",
  "scripts": {
    "test": "npx jest --verbose",
    "coverage": "npx jest --coverage",
    "build": "npm run clean && npm run compile",
    "clean": "rm -rf ./dist && rm -rf ./typings"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": ["ts", "tsx", "js"],
    "testPathIgnorePatterns": ["/node_modules/", "/examples/"]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hufeng/plume.git"
  },
  "keywords": ["preact", "immutable", "flux", "mobile"],
  "files": ["package.json", "dist", "README.md", "typings"],
  "author": "hufeng",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hufeng/plume/issues"
  },
  "homepage": "https://github.com/hufeng/plume#readme",
  "dependencies": {
    "@types/jest": "^20.0.4",
    "@types/node": "^8.0.14",
    "@types/react": "^15.0.38",
    "@types/react-dom": "^15.5.1",
    "immutable": "^3.8.1",
    "mitt": "^1.1.2"
  },
  "devDependencies": {
    "babel-jest": "^20.0.3",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "jest": "^20.0.4",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-test-renderer": "^15.6.1",
    "ts-jest": "^20.0.7",
    "typescript": "^2.4.2"
  }
}
