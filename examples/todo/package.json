{
  "name": "counter",
  "version": "1.0.0",
  "description": "plume2-starter",
  "main": "index.js",
  "scripts": {
    "test": "jest --verbose",
    "start": "./node_modules/.bin/webpack-dev-server",
    "clean": "rm -rf ./dist",
    "compile": "./node_modules/.bin/tsc && cp -R ./src/css ./build/",
    "watch": "./node_modules/.bin/tsc -w",
    "build:dll": "./node_modules/.bin/webpack --config webpack.dll.config.js --progress --color",
    "build": "./node_modules/.bin/webpack --config webpack.production.js --progress --color",
    "build:all": "npm run clean && npm run build:dll && npm run build"
  },
  "keywords": [
    "plume2",
    "starter"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/hufeng/plume2-starter.git"
  },
  "author": "hufeng",
  "license": "MIT",
  "dependencies": {
    "plume2": "^1.1.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "devDependencies": {
    "@types/node": "^8.0.14",
    "@types/react": "^15.0.38",
    "@types/react-dom": "^15.5.1",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "css-loader": "^0.28.4",
    "html-webpack-plugin": "^2.29.0",
    "loglevel": "^1.4.1",
    "style-loader": "^0.18.2",
    "ts-loader": "^2.3.1",
    "typescript": "2.9.2",
    "webpack": "^3.3.0",
    "webpack-dev-server": "^2.6.0"
  }
}
